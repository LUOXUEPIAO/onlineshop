se{\n          this.isLogin = true;\n        }\n        console.log(to.path);\n      }\n    },\n    methods: {\n      doLogout(){\n       \n        this.$http.post('api/logout',{}).then(response => {\n                            //跳转首页\n                            if (response.data.resultCode == 1) {\n                                this.$message({\n                                    message: '已经注销!',\n                                    type: 'success'\n                                });\n                                sessionStorage.removeItem(\"loginUser\");\n                                this.isLogin = false;\n                                this.$router.push('/');\n                            } else {\n                                console.log('logout failed:' + response.data.msg);\n                                \n                                this.$message.error('注销失败!');\n                            }\n                        })\n      }\n    },\n  }\n</script>"],"sourceRoot":"src"}]}